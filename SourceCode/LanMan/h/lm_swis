#define LanMan98SWIBase		(0x52680)
#define LanMan98_OmniOp		(LanMan98SWIBase + 0)
#define LanMan98_Bad1		(LanMan98SWIBase + 1)
#define LanMan98_Bad2		(LanMan98SWIBase + 2)
#define LanMan98_Bad3		(LanMan98SWIBase + 3)
#define LanMan98_LogonOp	(LanMan98SWIBase + 4)
#define LanMan98_PollWord	(LanMan98SWIBase + 5)
