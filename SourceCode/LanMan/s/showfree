	GET h.SWINames

        AREA    |C$$code|, CODE, READONLY
|x$codeseg|

        EXPORT ShowFreeReg
        EXPORT ShowFreeDeReg

ShowFreeReg	;void (int fs)
	STMFD	R13!, {R14}
	ADR	R1, handler
	MOV	R2, #0
	SWI	XFree_Register
 [ ATPCS32
	MSR	CPSR_f, #0
	LDMFD	R13!, {PC}
 |
	LDMFD	R13!, {PC}^
 ]

ShowFreeDeReg	;void (int fs)
	STMFD	R13!, {R14}
	ADR	R1, handler
	MOV	R2, #0
	SWI	XFree_DeRegister
 [ ATPCS32
	MSR	CPSR_f, #0
	LDMFD	R13!, {PC}
 |
	LDMFD	R13!, {PC}^
 ]

handler
	CMP	R0, #3
	SWINE	0x52681+(1<<17)
	LDMFD	R13!, {PC}
	END
