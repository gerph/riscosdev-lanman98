#ifndef _FSYS_ERR_
#define _FSYS_ERR_

#define ERR_DiscNotFound    0x100D4,"Disc not found"
#define ERR_NotFound    0x100D6,"Not found"
#define ERR_DiscFull    0x100C6,"Disc full"
#define ERR_Locked      0x100C3,"Locked"
#define ERR_DirNotEmpty 0x100B4,"Directory not empty"
#define ERR_DirFull     0x100B3,"Directory full"
#define ERR_BadName     0x100CC,"Bad name"
#define ERR_FileOpen    0x100C2,"File open"
#define ERR_BadWrt      0x100FC|0x7600,"Invalid address"


#endif
